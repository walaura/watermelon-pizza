{
	"name": "watermelon-pizza",
	"version": "1.0.0",
	"description": "My personal website!!",
	"dependencies": {
		"@babel/core": "^7.0.0-beta.49",
		"@babel/preset-env": "^7.0.0-beta.49",
		"babel-loader": "^8.0.0-beta.3",
		"concurrently": "^3.5.1",
		"express": "^4.16.3",
		"extract-zip": "^1.6.7",
		"fs-extra": "^6.0.1",
		"postcss": "^6.0.22",
		"postcss-clean": "^1.1.0",
		"postcss-cli": "^5.0.0",
		"postcss-import": "^11.1.0",
		"postcss-nesting": "^5.0.0",
		"sass": "^1.3.2",
		"webpack": "^4.9.1",
		"webpack-cli": "^2.1.4"
	},
	"scripts": {
		"install": "npm run make",
		"make":
			"concurrently \"npm run static\" \"npm run css -- -m false\" \"npm run js\" \"node etc/unpack.js\"",
		"watch":
			"concurrently \"npm run css -- -w\" \"npm run js -- -w\" \"npm run static\" \"npm run express\"",
		"css": "postcss css/app.css -o out/watermelon.css",
		"js": "webpack",
		"static": "node etc/build.js",
		"express": "node app.js"
	},
